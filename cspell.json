{
  "version": "0.2",
  "language": "it",
  "useGitignore": true,
  "import": [
    "@cspell/dict-en_us/cspell-ext.json",
    "@cspell/dict-it-it/cspell-ext.json",
    "@cspell/dict-latex/cspell-ext.json",
    "@cspell/dict-markdown/cspell-ext.json",
    "@cspell/dict-npm/cspell-ext.json"
  ],
  "ignorePaths": [
    "**/build/",
    "**/node_modules/",
    "**/package-lock.json",
    "package.json",
    "README.md",
    "bibliography.bib",
    "cspell.json",
    "latexmkrc",
    "LICENSE"
  ],
  "ignoreRegExpList": ["/%.*$/gm"],
  "words": [
    "Author",
    "booktitle",
    "gamification",
    "howpublished",
    "latexmk",
    "Mattiolo",
    "Michael",
    "misc",
    "nonstopmode",
    "outdir",
    "postbuild",
    "Unity"
  ]
}
